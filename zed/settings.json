{
  "features": {
    "inline_completion_provider": "none"
  },
  "collaboration_panel": {
    "dock": "right"
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620"
    },
    "version": "2"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "autosave": "off",
  "vim_mode": true,
  "relative_line_numbers": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "Rosé Pine Dawn",
    "dark": "Rosé Pine"
  },
  "confirm_quit": true,
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "inlay_hints": {
    "enabled": true
  },
  "preview_tabs": {
    "enable_preview_from_file_finder": true
  },
  "calls": {
    "mute_on_join": true
  }
}
